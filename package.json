{
  "name": "fastify-test",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "index.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "start": "npm run build:ts && node dist/src/index.js",
    "build:ts": "babel src --extensions \".js,.ts\" --out-dir dist/src --copy-files",
    "watch:ts": "tsnd -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn ./src/index.ts",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "node dist/src/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/sensible": "^4.1.0",
    "dotenv": "^16.0.1",
    "fastify": "^4.0.0",
    "fastify-cli": "^4.3.0",
    "fastify-plugin": "^3.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.5",
    "@types/node": "^18.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "concurrently": "^7.0.0",
    "fastify-tsconfig": "^1.0.1",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.5.4"
  }
}
